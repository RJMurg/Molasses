{
  "name": "Molasses",
  "action": {},
  "manifest_version": 3,
  "version": "1.0",
  "description": "Force dark mode on TU Dublin's Clubs and Socs portal.",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "host_permissions": [
    "https://mystudentlife.tudublin.ie/",
    "https://societies.tudublin.ie/",
    "https://sport.tudublin.ie/"
  ],
  "background": {
    "service_worker": "service-worker.js"
  }
}